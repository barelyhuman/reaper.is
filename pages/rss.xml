<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
  <title>{{.Data.name}}</title>
  <link>{{.Data.link}}</link>
  <description>{{.Data.description}}</description>
  <atom:link href="{{.Data.link}}" rel="self" type="application/rss+xml" />
      {{range .Data.writings}}
      <item>
      <guid>{{.slug}}</guid>
      <title>{{.title}}</title>
      <link>{{print "https://reaper.is/writing/" .slug}}</link>
      <description>
        <![CDATA[{{.content}}]]>
      </description>
      <pubDate>{{.formatteddate}}</pubDate>
    </item>
      {{end}}
</channel>
</rss>